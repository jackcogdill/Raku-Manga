{
    "env": {
        "browser": true,
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
    },
    "rules": {
        "no-console": 0,
        "object-curly-newline": 0,
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "react/jsx-indent": ["error", 4, { "SwitchCase": 1 }],
        "react/jsx-indent-props": ["error", 4, { "SwitchCase": 1 }],
    },
}
